{
  "expo": {
    "name": "ExpenseTracker",
    "slug": "expensetracker",
    "version": "1.0.0",
    "icon": "./assets/icon/icon.png",
    "splash": {
      "image": "./assets/icon/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-sqlite",
      "expo-asset",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 34
          }
        }
      ]
    ],
    "android": {
      "package": "com.anonymous.expensetracker",
      "minSdkVersion": 34,
      "permissions": [
        "READ_SMS",
        "RECEIVE_SMS"
      ]
    }
  }
}
